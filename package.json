{
  "name": "@wearereasonablepeople/peril-settings",
  "version": "0.0.1",
  "scripts": {
    "start": "node src/index.js",
    "test": "run-s licenses lint jest",
    "jest": "jest",
    "lint": "eslint .",
    "readme": "jsdoc2md -m none -g none -t README.md.hbs org/all-prs.js > README.md",
    "commit": "git-cz",
    "c": "npm run commit",
    "depcheck": "depcheck --specials eslint,bin --parsers='*.js:es7' --ignores=cz-conventional-changelog",
    "licenses": "license-checker --production --onlyAllow 'MIT;ISC;BSD-2-Clause;BSD-3-Clause;Apache-2.0;CC0-1.0;CC-BY-3.0;Unlicense'"
  },
  "keywords": [],
  "license": "MIT",
  "devDependencies": {
    "conventional-changelog-cli": "^2.0.11",
    "cz-conventional-changelog": "^2.1.0",
    "danger": "^3.9.0",
    "depcheck": "^0.6.11",
    "eslint": "^5.13.0",
    "eslint-config-warp": "^3.0.0",
    "eslint-plugin-jest": "^21.27.2",
    "git-cz": "^1.8.0",
    "is-verb": "^1.0.0",
    "jest": "^23.6.0",
    "jsdoc-to-markdown": "^4.0.1",
    "license-checker": "^20.2.0",
    "npm-run-all": "^4.1.5",
    "ramda": "^0.25.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
